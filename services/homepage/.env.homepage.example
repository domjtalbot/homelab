# Homepage environment variables
# Copy to .env.homepage and customize

# Web UI port (external port mapped to container)
HOMEPAGE_WEB_PORT=3002

# Cron status external URL (for browser access)
HOMEPAGE_VAR_CRON_STATUS_URL=https://cron-status.example.com

# Cron status internal URL (for Homepage container access)
HOMEPAGE_VAR_CRON_STATUS_INTERNAL_URL=http://homelab-ofelia-status
