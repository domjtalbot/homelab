# Infisical Environment Example
# Copy to .env.infisical and fill in values

# Platform encryption key (32+ chars, base64 recommended)
ENCRYPTION_KEY=changemechangemechangemechangeme
# Auth secret for JWT/session (32+ chars, base64 recommended)
AUTH_SECRET=changemeauthsecret

# Postgres settings
POSTGRES_USER=infisical
POSTGRES_PASSWORD=infisical
POSTGRES_DB=infisical
DB_CONNECTION_URI=postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@db:5432/${POSTGRES_DB}

# Redis
REDIS_URL=redis://redis:6379

# Web
INFISICAL_PORT=8086
BASE_DOMAIN=yourdomain.com
